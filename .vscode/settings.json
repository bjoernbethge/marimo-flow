{
    "python.defaultInterpreterPath": "./.venv/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.autoSearchPaths": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.stubPath": "./.venv/Lib/site-packages",
    "python.analysis.extraPaths": [
        "./.venv/Lib/site-packages",
        "./src",
        "./agents/src"
    ],
    "python.analysis.include": [
        "**/*.py"
    ],
    "python.analysis.exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.git",
        "**/.venv",
        "**/venv",
        "**/env"
    ],
    "files.associations": {
        "*.py": "python",
        "*.pyi": "python",
        "*.pyx": "python",
        "*.pyw": "python",
        "*.pyz": "python",
        "*.toml": "toml",
        "*.lock": "toml",
        "pyproject.toml": "toml",
        "uv.lock": "toml",
        "Cargo.toml": "toml"
    },
    "python.linting.enabled": true,
    "python.linting.ruffEnabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": false,
    "python.linting.mypyEnabled": true,
    "python.formatting.provider": "black",
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.fixAll.ruff": "explicit",
            "source.organizeImports.ruff": "explicit",
            "source.fixAll.mypy": "explicit"
        },
        "editor.rulers": [88, 120],
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.trimAutoWhitespace": true,
        "editor.trimFinalNewlines": true,
        "editor.insertFinalNewline": true
    },
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml",
        "editor.formatOnSave": true,
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.rulers": [88, 120]
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.wordWrap": "on"
    },
    "ruff.enable": true,
    "ruff.interpreter": [
        "./.venv/Scripts/python.exe"
    ],
    "ruff.path": [
        "./.venv/Scripts/ruff.exe"
    ],
    "ruff.importStrategy": "fromEnvironment",
    "ruff.showNotifications": "onError",
    "ruff.organizeImports": true,
    "ruff.lint.enable": true,
    "ruff.format.enable": true,
    "ruff.fixAll": true,
    "ruff.codeAction.fixViolation": {
        "enable": true
    },
    "ruff.codeAction.disableRuleComment": {
        "enable": true
    },
    "ruff.args": [
        "--target-version=py39",
        "--line-length=88",
        "--select=E,W,F,I,B,C4,UP,N,S,T20,RET,SIM,TCH",
        "--ignore=E501,B008,S101,T201,T203",
        "--per-file-ignores=__init__.py:F401,tests/**/*.py:S101,T201,T203",
        "--isort-known-first-party=marimo_flow,agents"
    ],
    "black-formatter.args": [
        "--line-length=88",
        "--target-version=py39",
        "--include=\\.pyi?$",
        "--extend-exclude=/(\\.eggs|\\.git|\\.hg|\\.mypy_cache|\\.tox|\\.venv|_build|buck-out|build|dist|mlruns|models|raw)/"
    ],
    "mypy-type-checker.enable": true,
    "mypy-type-checker.interpreter": [
        "./.venv/Scripts/python.exe"
    ],
    "mypy-type-checker.path": [
        "./.venv/Scripts/mypy.exe"
    ],
    "mypy-type-checker.importStrategy": "fromEnvironment",
    "mypy-type-checker.showNotifications": "onError",
    "mypy-type-checker.args": [
        "--python-version=3.9",
        "--warn-return-any",
        "--warn-unused-configs",
        "--disallow-untyped-defs",
        "--disallow-incomplete-defs",
        "--check-untyped-defs",
        "--disallow-untyped-decorators",
        "--no-implicit-optional",
        "--warn-redundant-casts",
        "--warn-unused-ignores",
        "--warn-no-return",
        "--warn-unreachable",
        "--strict-equality",
        "--ignore-missing-imports"
    ],
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [
        "--minversion=7.0",
        "-ra",
        "-q",
        "--strict-markers",
        "--strict-config",
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.autoTestDiscoverOnSaveEnabled": true,
    "python.testing.cwd": ".",
    "coverage-gutters.coverageFileNames": [
        ".coverage",
        "coverage.xml",
        "coverage.json"
    ],
    "coverage-gutters.coverageBaseDir": ".",
    "coverage-gutters.showGutterCoverage": true,
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "python.terminal.executeInFileDir": false,
    "python.terminal.activateEnvironmentInTerminal": true,
    "python.terminal.launchArgs": [],
    "python.analysis.completeFunctionParens": true,
    "python.analysis.diagnosticSeverityOverrides": {
        "reportGeneralTypeIssues": "warning",
        "reportOptionalMemberAccess": "warning",
        "reportOptionalSubscript": "warning",
        "reportPrivateImportUsage": "warning"
    },
    "cSpell.words": [
        "marimo",
        "pydantic",
        "mlflow",
        "ruff",
        "mypy",
        "pre-commit",
        "pytest",
        "uv",
        "pyproject",
        "toml",
        "duckdb",
        "polars",
        "altair",
        "openvino",
        "ollama",
        "huggingface",
        "transformers",
        "torch",
        "pytorch",
        "geometric",
        "tensordict",
        "astro",
        "cosmos",
        "vega"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true,
        "**/.ruff_cache": true,
        "**/node_modules": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "search.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true,
        "**/.ruff_cache": true,
        "**/node_modules": true,
        "**/.git": true,
        "**/mlruns": true,
        "**/models": true,
        "**/raw": true
    },
    "files.watcherExclude": {
        "**/__pycache__/**": true,
        "**/.pytest_cache/**": true,
        "**/.mypy_cache/**": true,
        "**/.ruff_cache/**": true,
        "**/node_modules/**": true,
        "**/.git/**": true,
        "**/mlruns/**": true,
        "**/models/**": true,
        "**/raw/**": true
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.ruff": "explicit",
        "source.organizeImports.ruff": "explicit"
    },
    "editor.rulers": [88, 120],
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.trimAutoWhitespace": true,
    "editor.trimFinalNewlines": true,
    "editor.insertFinalNewline": true,
    "editor.renderWhitespace": "boundary",
    "editor.wordWrap": "off",
    "editor.minimap.enabled": true,
    "editor.minimap.showSlider": "always",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.suggestSelection": "first",
    "editor.acceptSuggestionOnEnter": "on",
    "editor.tabCompletion": "on",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.suggestOnTriggerCharacters": true,
    "editor.parameterHints.enabled": true,
    "editor.hover.enabled": true,
    "editor.codeLens": true,
    "editor.folding": true,
    "editor.foldingStrategy": "indentation",
    "editor.showFoldingControls": "always",
    "editor.unfoldOnClickAfterEnd": false,
    "editor.linkedEditing": true,
    "editor.stickyScroll.enabled": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.wrapTabs": true,
    "workbench.editor.decorations.badges": true,
    "workbench.editor.decorations.colors": true,
    "workbench.editor.labelFormat": "short",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Cursor Dark",
    "workbench.iconTheme": "vs-seti",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.env.windows": {
        "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/src;${workspaceFolder}/agents/src"
    },
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "jupyter.enableScrollingForCellOutputs": true,
    "jupyter.enableAutoSave": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "jupyter.enableScrollingForCellOutputs": true,
    "jupyter.enableAutoSave": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "jupyter.enableScrollingForCellOutputs": true,
    "jupyter.enableAutoSave": true
}
